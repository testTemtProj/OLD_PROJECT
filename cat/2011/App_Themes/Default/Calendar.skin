    <asp:Calendar runat="server" style="margin:6px;">
    <SelectedDayStyle BackColor="Blue" />
    <TodayDayStyle BackColor="#999999" ForeColor="White" />
    <DayStyle BackColor="WhiteSmoke" />
    <OtherMonthDayStyle ForeColor="#999999" />
    <NextPrevStyle Font-Bold="True" Font-Size="8pt" ForeColor="White" />
    <DayHeaderStyle Font-Bold="True" Font-Size="8pt" ForeColor="#333333" Height="8pt" />
    <TitleStyle BackColor="#469CFF" Font-Bold="True" Font-Size="12pt" ForeColor="White" Height="12pt" />
    <DayStyle Font-Underline="False" CssClass="calendar" />
    </asp:Calendar>